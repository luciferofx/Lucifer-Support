  "repository": {
    "type": "git",
    "url": "git+https://github.com/notbojji/mediacord.git"
  },
  "files": [
    "dist/**/*.js*",
    "dist/**/*.mjs*",
    "dist/**/*.d*"
  ],
  "devDependencies": {
