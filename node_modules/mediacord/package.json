{
  "name": "mediacord",
  "version": "1.2.1",
  "description": "Get random images and gifs of anime, cats and dogs!",
  "main": "dist/index.js",
  "author": "imluwi",
  "license": "MIT",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsup src && tsc -b src",
    "start": "yarn build && node .",
    "test": "yarn build && node dist/testing.js"
