  },
  "engines": {
    "node": ">=v14.0.0",
    "npm": ">=7.0.0"
  },
  "keywords": [
    "mediacord",
    "discord",
    "images",
    "gifs",
    "media",
    "multimedia",
    "anime",
    "anime gifs",
    "anime images",
    "nsfw",
    "sfw",
    "cat",
    "dog"
